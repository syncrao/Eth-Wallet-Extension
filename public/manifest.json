{
  "manifest_version": 3,
  "name": "Ethereum Wallet",
  "version": "1.0",
  "description": "Simple Ethereum wallet built with React and ethers.js",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["storage"]
}
